<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	<record id="website_support_warehouse_view_form" model="ir.ui.view">
    		<field name="name">website.support.warehouse.form.view</field>
          	<field name="model">website.support.warehouse</field>
          	<field name="arch" type="xml">
          		<form>
          			<group>
          				<field name="warehouse"/>
          			</group>
          		</form>
          	</field>
        </record>

        <record id="website_support_warehouse_view_tree" model="ir.ui.view">
        	<field name="name">website.support.warehouse.tree.view</field>
          <field name="model">website.support.warehouse</field>
          <field name="arch" type="xml">
      	      <tree>
      	          <field name="warehouse"/>      	          
      	      </tree>
      	  </field>
      	</record>

      	<record id="website_support_warehouse_action" model="ir.actions.act_window">
          <field name="name">Areas</field>
      	  <field name="res_model">website.support.warehouse</field>
      	  <field name="view_type">form</field>
      	  <field name="view_mode">tree,form</field>
      	  <field name="help" type="html">
      	  	<p class="oe_view_nocontent_create">Crear area</p>
	  	  </field>
        </record>

    </data>
</odoo>      	